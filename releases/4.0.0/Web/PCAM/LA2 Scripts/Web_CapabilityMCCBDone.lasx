---[FINAL_CSV]---
BEGIN FINAL_CSV
	---Begin Main Query---
	Using CSV with
	SELECT
		Process,
		ID,
		MCCB,
		Location,
		Contact,
		owner,
		[MCCB Description],
		Area,
		CompletedOn,
		Comment
	USING
		SUBSTR(CommentTmp, 0, 100) as CommentTmp2,
		REPLACE_STR(CommentTmp2, '\u000D', ' ') as Comment
	FROM
		%{MERGE3_CSV}
	WHERE
		IsInList(State, '${CHANGETYPE}')
END FINAL_CSV
---[CRUNCH_CSV]---
BEGIN CRUNCH_CSV
	UseMethod SortTable with -- **Sort Table (with support for multiple orderbys)**
		InputFile = "%{FINAL_CSV}" -- Input Table csv [String]
		OrderBy = "CompletedOn desc" -- Order By string (e.g., DateTime desc, Name asc, Metrics desc) [String]
END CRUNCH_CSV
---Begin Main Query---
Using CSV with
SELECT
	ID,
	MCCB,
	Location,
	Process,
	Area,
	Contact,
	[MCCB Description],
	Comment,
	CompletedOn
FROM
	%{CRUNCH_CSV}
WHERE
	applytimerangefilter(CompletedOn, 'Last 1 months')
	and
	(isinlist(owner,'%Capability%')	or	isinlist(Contact,'${COMembers}'))

