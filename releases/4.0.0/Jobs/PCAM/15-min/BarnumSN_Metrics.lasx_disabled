BEGIN MAIN_CSV
	---Begin Main Query---
	Using CSV with
	SELECT
		assigned_to_person as Resource,
		to_string(created_on, 'yyyy') as CompYear,
		count(assigned_to_person) as BarnumsCompleted
	FROM
		%{OUTPUT}
	WHERE
		CompYear is not null
		and Resource is not null
		and incident_state not like 'rejected'
		AND NOT IsInList(approval, 'rejected, requested')
	GROUP BY
		Resource,CompYear
	ORDER BY
		CompYear desc
END MAIN_CSV
