    <QuerySQL>----------&lt;VARIABLES&gt;------------
var TIMEFILTER = "this year"
---Begin Main Query---
SELECT
	assigned_to_person as Resource,
	count(*) as Barnums
FROM
	%{OUTPUT}
WHERE
	ApplyTimeRangeFilter(resolved_at, '${TIMEFILTER}')
	and incident_state not like 'rejected'
	AND NOT IsInList(approval, 'rejected, requested')
GROUP BY
	assigned_to_person
ORDER BY
	Barnums asc
</QuerySQL>
    <ColumnX FieldName="Resource" FieldType="String" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="true" SortBySummaryInfoFieldName="Barnums" />
    <ColumnY FieldName="" FieldType="" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="false" />
    <ColumnData FieldName="Barnums" FieldType="Int32" SortMode="Default" SortOrder="Ascending" SummaryType="Average" SortBySummaryInfo="false" />
    <TopN>0</TopN>
    <XaxisLabelSpacing>1</XaxisLabelSpacing>
    <DateTimeMeasureUnit>Hour</DateTimeMeasureUnit>
    <RotateXAxisLabels>false</RotateXAxisLabels>
    <Name>Resource Leaderboard - This Year</Name>
    <ChartTitle>Resource Leaderboard - This Year</ChartTitle>
