  <MainPivotConfig Enable="true">
    <QuerySQL>----------&lt;VARIABLES&gt;------------
var TIMEFILTER = "this year"
var TIMEFILTER2 = "last 1 week (prev mode)"
---[IBART_CSV]---
BEGIN IBART_CSV
	---Begin Main Query---
	SELECT
		WW,
		DIV(TotalBarnums,40) as IBART
	USING
		to_la2date(created_on, 'ww') as WW,
		to_real(count(created_on)) as TotalBarnums
	FROM
		%{OUTPUT}
	WHERE
		ApplyTimeRangeFilter(created_on, '${TIMEFILTER}')
		AND ApplyEndTimeFilter(created_on, '${TIMEFILTER2}')
		AND NOT IsInList(approval, 'rejected, requested')
	GROUP BY
		WW
END IBART_CSV
---[OBART_CSV]---
BEGIN OBART_CSV
	---Begin Main Query---
	SELECT
		WW,
		DIV(TotalBarnums,40) as OBART
	USING
		to_la2date(resolved_at, 'ww') as WW,
		to_real(count(created_on)) as TotalBarnums
	FROM
		%{OUTPUT}
	WHERE
		ApplyTimeRangeFilter(resolved_at, '${TIMEFILTER}')
		AND ApplyEndTimeFilter(resolved_at, '${TIMEFILTER2}')
		and incident_state not like 'rejected'
		AND NOT IsInList(approval, 'rejected, requested')
	GROUP BY
		WW
END OBART_CSV
---[RESOURCE_CSV]---
BEGIN RESOURCE_CSV
	---Begin Main Query---
	SELECT
		distinct
		WW,
		assigned_to_person
	USING
		to_la2date(resolved_at, 'ww') as WW
	FROM
		%{OUTPUT}
	WHERE
		ApplyTimeRangeFilter(resolved_at, '${TIMEFILTER}')
		AND ApplyEndTimeFilter(resolved_at, '${TIMEFILTER2}')
		and incident_state not like 'rejected'
		and assigned_to_person is not null
END RESOURCE_CSV
---[RESOURCE2_CSV]---
BEGIN RESOURCE2_CSV
	---Begin Main Query---
	SELECT
		WW,
		count(assigned_to_person) as Resources
	FROM
		%{RESOURCE_CSV}
	GROUP BY
		WW
END RESOURCE2_CSV
---[REQUESTOR_CSV]---
BEGIN REQUESTOR_CSV
	---Begin Main Query---
	SELECT
		distinct
		WW,
		requestor
	USING
		to_la2date(resolved_at, 'ww') as WW
	FROM
		%{OUTPUT}
	WHERE
		ApplyTimeRangeFilter(resolved_at, '${TIMEFILTER}')
		AND ApplyEndTimeFilter(resolved_at, '${TIMEFILTER2}')
		and incident_state not like 'rejected'
		and requestor is not null
END REQUESTOR_CSV
---[REQUESTOR2_CSV]---
BEGIN REQUESTOR2_CSV
	---Begin Main Query---
	SELECT
		WW,
		count(requestor) as Customers
	FROM
		%{REQUESTOR_CSV}
	GROUP BY
		WW
END REQUESTOR2_CSV
---[JOIN_CSV]---
BEGIN JOIN_CSV
	UseMethod Join with -- **Do an outer join for two tables based on a matching column**
		InputFile1 = "%{IBART_CSV}" -- Input CSV File #1 containing table data [String]
		InputFile2 = "%{OBART_CSV}" -- Input CSV File #2 containing table data [String]
		JoinColumn = "WW" -- Column name(s) on which to join the two tables (more than one column can be supplied by using comma as a separator) [String]
		IgnoreDuplicate = false -- Flag to ignore duplicate items in the JoinColumn [Boolean]
		IgnoreCase = false -- Flag to ignore case in the JoinColumn [Boolean]
		OnlyShowNonMatchingRows = false -- Flag to only show non-matching orphan rows [Boolean]
		IncludeNonMatchingRows = true -- Flag to also include non-matching orphan rows (but with blank entries) [Boolean]
		MustIncludeOutputColumns = "" -- Name of columns (comma-separated) that must be included in output even if the join does not materialize [String]
		OrderBy = "" -- Name of column(s) to sort the resulting table by [String]
END JOIN_CSV
---[JOIN2_CSV]---
BEGIN JOIN2_CSV
	UseMethod Join with -- **Do an outer join for two tables based on a matching column**
		InputFile1 = "%{RESOURCE2_CSV}" -- Input CSV File #1 containing table data [String]
		InputFile2 = "%{REQUESTOR2_CSV}" -- Input CSV File #2 containing table data [String]
		JoinColumn = "WW" -- Column name(s) on which to join the two tables (more than one column can be supplied by using comma as a separator) [String]
		IgnoreDuplicate = false -- Flag to ignore duplicate items in the JoinColumn [Boolean]
		IgnoreCase = false -- Flag to ignore case in the JoinColumn [Boolean]
		OnlyShowNonMatchingRows = false -- Flag to only show non-matching orphan rows [Boolean]
		IncludeNonMatchingRows = true -- Flag to also include non-matching orphan rows (but with blank entries) [Boolean]
		MustIncludeOutputColumns = "" -- Name of columns (comma-separated) that must be included in output even if the join does not materialize [String]
		OrderBy = "" -- Name of column(s) to sort the resulting table by [String]
END JOIN2_CSV
---[JOIN3_CSV]---
BEGIN JOIN3_CSV
	UseMethod Join with -- **Do an outer join for two tables based on a matching column**
		InputFile1 = "%{JOIN_CSV}" -- Input CSV File #1 containing table data [String]
		InputFile2 = "%{JOIN2_CSV}" -- Input CSV File #2 containing table data [String]
		JoinColumn = "WW" -- Column name(s) on which to join the two tables (more than one column can be supplied by using comma as a separator) [String]
		IgnoreDuplicate = false -- Flag to ignore duplicate items in the JoinColumn [Boolean]
		IgnoreCase = false -- Flag to ignore case in the JoinColumn [Boolean]
		OnlyShowNonMatchingRows = false -- Flag to only show non-matching orphan rows [Boolean]
		IncludeNonMatchingRows = true -- Flag to also include non-matching orphan rows (but with blank entries) [Boolean]
		MustIncludeOutputColumns = "" -- Name of columns (comma-separated) that must be included in output even if the join does not materialize [String]
		OrderBy = "" -- Name of column(s) to sort the resulting table by [String]
END JOIN3_CSV
---Begin Main Query---
Using CSV with
SELECT
	WW,
	IBART,
	OBART,
	Net,
	Customers,
	Resources
USING
	SUB(OBART, IBART) as Net
FROM
	%{JOIN3_CSV}
</QuerySQL>
    <ColumnX FieldName="WW" FieldType="String" SortMode="Default" SortOrder="Ascending" SummaryType="Average" SortBySummaryInfo="false" />
    <ColumnY FieldName="" FieldType="" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="false" />
    <ColumnData FieldName="IBART, OBART, Net" FieldType="Double" SortMode="Default" SortOrder="Ascending" SummaryType="Average,Average,Average" SortBySummaryInfo="false" />
    <ChartType>Spline</ChartType>
    <ShowLabels>false</ShowLabels>
    <Name>Net BART (This Year)</Name>
    <ChartTitle>Net BART - This Year</ChartTitle>
    <ColorPalette>Office</ColorPalette>
    <ChartExportSize>525x300</ChartExportSize>
    <ThresholdLineValue>BreakEven:0</ThresholdLineValue>
    <NumericValueDecimalPrecision>2</NumericValueDecimalPrecision>
    <CustomPalette />
    <TableData />
  </MainPivotConfig>
  <MainPivotConfig Enable="true">
