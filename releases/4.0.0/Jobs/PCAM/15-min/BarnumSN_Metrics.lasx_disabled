    <SQL>----------&lt;VARIABLES&gt;------------
var TIMEFILTER = "last 1 year (prev mode)"
---[IBART_CSV]---
BEGIN IBART_CSV
	---Begin Main Query---
	SELECT
		WW,
		DIV(TotalBarnums,40) as IBART
	USING
		to_la2date(created_on, 'ww') as WW,
		to_real(count(created_on)) as TotalBarnums
	FROM
		%{OUTPUT}
	WHERE
		ApplyTimeRangeFilter(created_on, '${TIMEFILTER}')
		AND NOT IsInList(approval, 'rejected, requested')
	GROUP BY
		WW
END IBART_CSV
---[OBART_CSV]---
BEGIN OBART_CSV
	---Begin Main Query---
	SELECT
		WW,
		DIV(TotalBarnums,40) as OBART
	USING
		to_la2date(resolved_at, 'ww') as WW,
		to_real(count(created_on)) as TotalBarnums
	FROM
		%{OUTPUT}
	WHERE
		ApplyTimeRangeFilter(resolved_at, '${TIMEFILTER}')
		and incident_state not like 'rejected'
		AND NOT IsInList(approval, 'rejected, requested')
	GROUP BY
		WW
END OBART_CSV
---[RESOURCE_CSV]---
BEGIN RESOURCE_CSV
	---Begin Main Query---
	SELECT
		distinct
		WW,
		assigned_to_person
	USING
		to_la2date(resolved_at, 'ww') as WW
	FROM
		%{OUTPUT}
	WHERE
		ApplyTimeRangeFilter(resolved_at, '${TIMEFILTER}')
		and incident_state not like 'rejected'
		and assigned_to_person is not null
END RESOURCE_CSV
---[RESOURCE2_CSV]---
BEGIN RESOURCE2_CSV
	---Begin Main Query---
	SELECT
		WW,
		count(assigned_to_person) as Resources
	FROM
		%{RESOURCE_CSV}
	GROUP BY
		WW
END RESOURCE2_CSV
---[REQUESTOR_CSV]---
BEGIN REQUESTOR_CSV
	---Begin Main Query---
	SELECT
		distinct
		WW,
		requestor
	USING
		to_la2date(resolved_at, 'ww') as WW
	FROM
		%{OUTPUT}
	WHERE
		ApplyTimeRangeFilter(resolved_at, '${TIMEFILTER}')
		and incident_state not like 'rejected'
		and requestor is not null
END REQUESTOR_CSV
---[REQUESTOR2_CSV]---
BEGIN REQUESTOR2_CSV
	---Begin Main Query---
	SELECT
		WW,
		count(requestor) as Customers
	FROM
		%{REQUESTOR_CSV}
	GROUP BY
		WW
END REQUESTOR2_CSV
---[JOIN_CSV]---
BEGIN JOIN_CSV
	UseMethod Join with -- **Do an outer join for two tables based on a matching column**
