var TIMEFILTER = "This Year"
---Begin Main Query---
SELECT
	change_type as ChangeType,
	created_on as Requestdate,
	u_process as Process
FROM
	%{OUTPUT}
WHERE
	(to_string(RequestDate, 'dddd') like 'S%') AND
	ApplyTimeRangeFilter(RequestDate, '${TIMEFILTER}')
	AND NOT IsInList(approval, 'rejected, requested')
</QuerySQL>
