    <SQL>----------&lt;VARIABLES&gt;------------
var TIMEFILTER = "this week"
---Begin Main Query---
SELECT
	resolved_at as CompDate,
	BarnumNumber,
	short_description as Title,
	u_process as Process,
	u_functional_area as Area,
	requestor as Requestor,
	assigned_to_person as Resource
USING
	number as BarnumNumber
FROM
	%{OUTPUT}
WHERE
	incident_state not like 'rejected' AND
	ApplyTimeRangeFilter(CompDate, '${TIMEFILTER}')
	AND NOT IsInList(approval, 'rejected, requested')
ORDER BY
	CompDate desc
</SQL>
