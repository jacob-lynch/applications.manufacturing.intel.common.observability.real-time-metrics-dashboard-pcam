    <QuerySQL>----------&lt;VARIABLES&gt;------------
var TIMEFILTER = "this year"
---Begin Main Query---
SELECT
	to_string(created_on, 'dddd') as RequestDay,
	count(*) as BarnumsRequested
FROM
	%{OUTPUT}
WHERE
	RequestDay is not null AND
	ApplyTimeRangeFilter(created_on, '${TIMEFILTER}')
	AND NOT IsInList(approval, 'rejected, requested')
GROUP BY
	RequestDay
ORDER BY
	BarnumsRequested asc
</QuerySQL>
    <ColumnX FieldName="RequestDay" FieldType="String" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="true" SortBySummaryInfoFieldName="BarnumsRequested" />
    <ColumnY FieldName="" FieldType="" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="false" />
    <ColumnData FieldName="BarnumsRequested" FieldType="Int32" SortMode="Default" SortOrder="Ascending" SummaryType="Average" SortBySummaryInfo="false" />
    <XaxisLabelSpacing>1</XaxisLabelSpacing>
    <DateTimeMeasureUnit>Hour</DateTimeMeasureUnit>
    <Name>Current Year - Submitted Day</Name>
    <ChartTitle>Submitted Barnums by Day - This Year</ChartTitle>
    <ColorPalette>Office</ColorPalette>
    <ChartExportSize>525x300</ChartExportSize>
    <ThresholdLineValue />
    <AlwaysShowThresholdLines>true</AlwaysShowThresholdLines>
    <ShowLegend>false</ShowLegend>
    <IsYAxisVisible>false</IsYAxisVisible>
    <TimeIntervalForDateScale>1y</TimeIntervalForDateScale>
