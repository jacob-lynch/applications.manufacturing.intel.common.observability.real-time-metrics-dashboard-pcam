FROM
	%{OUTPUT}
WHERE
	NOT IsInList(name, '${MCCBAREAEXCLUDE}')
	AND Process like '1270'
	AND ChangeDate is not null
ORDER BY
	[Transfer Date]
</QuerySQL>
