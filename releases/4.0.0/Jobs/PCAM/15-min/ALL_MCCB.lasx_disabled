  <MainPivotConfig Enable="true">
    <QuerySQL>----------&lt;VARIABLES&gt;------------
var MCCBSITE = "@{MCCBSITE}"
var MCCBCHANGE = "@{MCCBCHANGE}"
var MCCBAREAEXCLUDE = "@{MCCBAREAEXCLUDE}"
var MCCBCOMPLETEDBY = "@{MCCBCOMPLETEDBY}"
---Begin Main Query---
Using CSV with
SELECT
	Process,
	${MCCBCHANGE} as State,
	${MCCBCOMPLETEDBY} as [Contact],
	Name as [MCCB Description],
	CEID,
	Area,
	SubArea,
	TxfrDate as [Transfer Date],
	ChangeType,
	Site
USING
	CASE MCCBSite when '1272' then '1272' else '1270' end as Process
FROM
	%{OUTPUT}
WHERE
	NOT IsInList(name, '${MCCBAREAEXCLUDE}')
	AND State is not null
	AND Process like '1272'
ORDER BY
	[Transfer Date]
</QuerySQL>
    <ColumnX FieldName="State" FieldType="String" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="false" />
    <ColumnY FieldName="" FieldType="" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="false" />
    <ColumnData FieldName="COUNT OF" FieldType="Int32" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="false" />
    <RotateXAxisLabels>false</RotateXAxisLabels>
    <UseLogarithmicScale>true</UseLogarithmicScale>
    <Name>1272 Status</Name>
    <ChartTitle>1272 MCCB Status - ${MCCBSITE}</ChartTitle>
    <ColorPalette>Median</ColorPalette>
    <ChartExportSize>800x400</ChartExportSize>
    <ShowLegend>false</ShowLegend>
    <IsYAxisVisible>false</IsYAxisVisible>
    <CustomPalette />
    <TableData />
  </MainPivotConfig>
