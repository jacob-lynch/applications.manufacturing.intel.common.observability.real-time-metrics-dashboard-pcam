---Begin Main Query---
Using CSV with
SELECT
	Process,
	Name as [MCCB Description],
	CEID,
	Area,
	SubArea,
	TxfrDate as [Transfer Date],
	ChangeType,
	SiteFinal,
	ChangeDate,
	Author
USING
	CASE MCCBSite when '1272' then '1272' when 'New' then '1270' when 'Old' then '1270' when '1270' then '1270' when '1274' then '1274' end as Process,
	CASE Site when 'F32' then 'OSA' else Site end as SiteFinal
FROM
	%{OUTPUT}
WHERE
	NOT IsInList(name, '${MCCBAREAEXCLUDE}')
	AND Process like '1270'
	AND applytimerangefilter([Transfer Date], 'last 12 months')
	and IsInList(SiteFinal, '${MCCBSITEFILTER}')
ORDER BY
	[Transfer Date]
</QuerySQL>
