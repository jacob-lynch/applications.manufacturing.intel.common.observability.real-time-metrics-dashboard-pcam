    <SQL>----------&lt;VARIABLES&gt;------------
var MCCBCOMPLETEDON = "@{MCCBCOMPLETEDON}"
---Begin Main Query---
SELECT
	[D1C Local Comments],
	[F24 Local Comments],
	[F28 Local Comments],
	[OSA Local Comments],
	[AFO Local Comments],
	[D1D Local Comments],
	[F11x Local Comments],
	[F68 Local Comments],
	AFOChange,
	AFOCompletedBy,
	AFOCompletedOn,
	AFODaysToDone,
	Area,
	Author,
	CEID,
	ChangeDate,
	ChangeType,
	D1CChange,
	D1CCompletedBy,
	D1CCompletedOn,
	D1CDaysToDone,
	D1DChange,
	D1DCompletedBy,
	D1DCompletedOn,
	D1DDaysToDone,
	E3,
	F11xChange,
	F11xCompletedBy,
	F11xCompletedOn,
	F11xDaysToDone,
	F24Change,
	F24CompletedOn,
	F24DaysToDone,
	F28Change,
	F28CompletedBy,
	F28CompletedOn,
	FSUI,
	ID,
	Level78Change,
	MCCBSite,
	MES300,
	Name,
	OSAChange,
	OSACompletedBy,
	OSACompletedOn,
	OSADaysToDone,
	SC,
	Site,
	SPC,
	SubArea,
	TWT,
	TxfrDate,
	TxfrTime,
	VFDueDate,
	WW,
	WIP,
	F24CompletedBy
FROM
	%{OUTPUT}
WHERE
	applytimerangefilter(${MCCBCOMPLETEDON}, 'last 1 month')
</SQL>
