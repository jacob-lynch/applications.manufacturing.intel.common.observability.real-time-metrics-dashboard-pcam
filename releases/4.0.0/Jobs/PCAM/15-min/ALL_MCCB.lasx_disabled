    <QuerySQL>----------&lt;VARIABLES&gt;------------
var MCCBSITE = "@{MCCBSITE}"
var MCCBSITE2 = "@{MCCBSITE2}"
var MCCBCHANGE = "@{MCCBCHANGE}"
var MCCBAREAEXCLUDE = "@{MCCBAREAEXCLUDE}"
var MCCBCOMPLETEDBY = "@{MCCBCOMPLETEDBY}"
---Begin Main Query---
Using CSV with
SELECT
	Process,
	Name as [MCCB Description],
	CEID,
	Area,
	SubArea,
	TxfrDate as [Transfer Date],
	ChangeType,
	Site,
	ChangeDate,
	Author
USING
	CASE MCCBSite when '1272' then '1272' else '1270' end as Process
FROM
	%{OUTPUT}
WHERE
	NOT IsInList(name, '${MCCBAREAEXCLUDE}')
	AND Process like '1272'
	AND (Site like '@{MCCBSITE}' OR Site like '@{MCCBSITE2}')
ORDER BY
	[Transfer Date]
</QuerySQL>
    <ColumnX FieldName="Author" FieldType="String" SortMode="Default" SortOrder="Descending" SummaryType="Count" SortBySummaryInfo="true" SortBySummaryInfoFieldName="COUNT OF" />
