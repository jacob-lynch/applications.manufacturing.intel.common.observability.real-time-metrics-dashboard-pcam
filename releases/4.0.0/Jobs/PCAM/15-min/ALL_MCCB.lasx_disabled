  <MainPivotConfig Enable="true">
    <QuerySQL>----------&lt;VARIABLES&gt;------------
var MCCBSITE = "@{MCCBSITE}"
var MCCBSITE2 = "@{MCCBSITE2}"
var MCCBCHANGE = "@{MCCBCHANGE}"
var MCCBAREAEXCLUDE = "@{MCCBAREAEXCLUDE}"
var MCCBCOMPLETEDBY = "@{MCCBCOMPLETEDBY}"
var MCCBSITEFILTER = "@{MCCBSITEFILTER}"
---Begin Main Query---
Using CSV with
SELECT
	Process,
	Name as [MCCB Description],
	CEID,
	Area,
	SubArea,
	TxfrDate as [Transfer Date],
	ChangeType,
	SiteFinal,
	ChangeDate,
	Author
USING
	CASE MCCBSite when '1272' then '1272' else '1270' end as Process,
	CASE Site when 'F32' then 'OSA' else Site end as SiteFinal
FROM
	%{OUTPUT}
WHERE
	NOT IsInList(name, '${MCCBAREAEXCLUDE}')
	AND Process like '1272'
	AND applytimerangefilter([Transfer Date], 'last 12 months')
	and IsInList(SiteFinal, '${MCCBSITEFILTER}')
ORDER BY
	[Transfer Date]
</QuerySQL>
    <ColumnX FieldName="Transfer Date" FieldType="String" SortMode="Default" SortOrder="Ascending" SummaryType="Sum" SortBySummaryInfo="false" />
    <ColumnY FieldName="SiteFinal" FieldType="String" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="false" />
    <ColumnData FieldName="COUNT OF" FieldType="Int32" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="false" />
    <XaxisLabelSpacing>4</XaxisLabelSpacing>
    <RotateXAxisLabels>false</RotateXAxisLabels>
    <ShowColumnTotals>false</ShowColumnTotals>
    <Name>1272 AuthorBy VF</Name>
    <ChartTitle>1272 MCCBs Authored by VF, per Quarter, last 12 months</ChartTitle>
    <ColorPalette>Median</ColorPalette>
    <ChartExportSize>1000x300</ChartExportSize>
    <IsYAxisVisible>false</IsYAxisVisible>
    <TimeIntervalForDateScale>1q</TimeIntervalForDateScale>
    <DateFormat>Auto</DateFormat>
    <CustomPalette />
    <TableData />
  </MainPivotConfig>
