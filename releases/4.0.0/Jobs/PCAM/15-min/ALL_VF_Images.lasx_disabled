---[POST]---
BEGIN POST
	---Begin Main Query---
	SELECT
		distinct
		WW,
		Resource
	USING
		to_la2date(Date, 'ww') as WW
	FROM
		%{output}
	WHERE
		ApplyTimeRangeFilter(Date, 'last 10 weeks (prev mode)')
		AND ApplyEndTimeFilter(Date, 'last 1 week (prev mode)')
		and Resource is not null
END POST
</QuerySQL>
