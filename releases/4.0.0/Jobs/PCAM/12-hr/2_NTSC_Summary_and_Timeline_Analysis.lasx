  <OutputDataGridConfig Name="FW Upgrades Pending">
    <SQL>SELECT
	NodeName,
	FunctionalArea,
	ProfiledFramework,
	ActiveFramework,
	FrameworkSync,
	UpgradeWhenIdle,
	ActivationReadyFlag,
	UpgradePendingDuration
USING
	TO_LOCALTIME(SYSTEM_TIMESTAMP()) as Now,
	to_int(SUB(Now, FrameworkInstallTime)) as Time,
	div(div(Time,60),60) as HoursUpgradePending,
	strcat(to_string(HoursUpgradePending),' Hour(s)') as UpgradePendingDuration
FROM
	%{OUTPUT}
WHERE
	ActivationReadyFlag is not null and
	FrameworkSync = 'False' 	and
	ActiveFramework not like 'SC.NET'
ORDER BY
	FunctionalArea,
	NodeName
</SQL>
