  <MainPivotConfig Enable="true">
    <QuerySQL>---Begin Main Query---
SELECT
	NodeName,
	ProfiledFramework,
	UpgradeWhenIdle,
	ActivationReadyFlag,
	UpgradeMethod
USING
	strcat(UpgradeWhenIdle, ActivationReadyFlag) as UpgradeType,
	case UpgradeType
	when 'DisabledTrue' then 'Manual'
	when 'DisabledTrueNoConfirm' then 'Manual'
	when 'DisabledAtIdleNoConfirm' then 'Manual'
	when 'YesTrue' then 'Manual'
	when 'YesTrueNoConfirm' then 'Manual'
	when 'Yes_FWOnlyTrue' then 'Manual'
	when 'Yes_FWOnlyTrueNoConfirm' then 'Manual'
	when 'YesAtIdleNoConfirm' then 'AtIdle'
	when 'Yes_FWOnlyAtIdleNoConfirm' then 'AtIdle'
	else 'Manual' end as UpgradeMethod
FROM
	%{OUTPUT}
WHERE
	ActivationReadyFlag is not null and
	FrameworkSync = 'False' 	and
	ActiveFramework not like 'SC.NET'
</QuerySQL>
    <ColumnX FieldName="UpgradeMethod" FieldType="String" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="false" />
    <ColumnY FieldName="" FieldType="" SortMode="Default" SortOrder="Ascending" SummaryType="Count" SortBySummaryInfo="false" />
