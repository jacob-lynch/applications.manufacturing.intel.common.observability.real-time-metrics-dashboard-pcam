  <MainPivotConfig Enable="true">
    <QuerySQL>---Begin Main Query---
SELECT
	distinct MachineControllerGroup as MachineControllerType, max(ActiveFramework) as FWVersion
FROM
	%{output}
WHERE
	MachineControllerGroup is not null and ActiveFramework not like 'SC.NET'
GROUP BY
	MachineControllerGroup
ORDER BY
	FWVersion,
	MachineControllerGroup
</QuerySQL>
