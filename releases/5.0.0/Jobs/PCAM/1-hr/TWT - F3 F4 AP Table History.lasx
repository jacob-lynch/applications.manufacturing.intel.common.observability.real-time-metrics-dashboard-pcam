---[MERGE1_CSV]---
BEGIN MERGE1_CSV
	UseMethod MergeTables with -- **Merge content of two tables by doing an outer join on the column schema (no join done here)**
		InputFile1 = "%{F3_CSV}" -- Input CSV File #1 containing table data [String]
		InputFile2 = "%{F4_CSV}" -- Input CSV File #2 containing table data [String]
		AutoDetectColumnTypes = false -- Auto-detect column types before merging [Boolean]
END MERGE1_CSV
---[MERGE_CSV]---
BEGIN MERGE_CSV
	UseMethod MergeTables with -- **Merge content of two tables by doing an outer join on the column schema (no join done here)**
		InputFile1 = "%{MERGE1_CSV}" -- Input CSV File #1 containing table data [String]
		InputFile2 = "%{AP_CSV}" -- Input CSV File #2 containing table data [String]
		AutoDetectColumnTypes = false -- Auto-detect column types before merging [Boolean]
END MERGE_CSV
---Begin Main Query---
Using CSV with
SELECT
	CHANGETIMESTAMP,
	WHO,
	AREA,
	TABLENAME,
	INTRO,
	ROUTE,
	ENTITYBEINGMON,
	EVENT,
	EVENTTYPE,
	OPERID,
	OPERTYPE,
	TWTYPE,
	TWUSEDFOR,
	CESTATE,
	TXFRDATE,
	COMMENTS,
	DELETED
FROM
	%{MERGE_CSV}
WHERE
	WHO not like 'SYSTEM'
	ORDER  BY
	CHANGETIMESTAMP DESC

