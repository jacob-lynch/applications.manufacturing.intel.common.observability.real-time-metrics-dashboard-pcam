  <QuerySQL><![CDATA[
--[FileName:"Y:\PCAM\1-hr\TWT - F3 F4 AP Table History.lasx"]------
----------<VARIABLES>------------
var LOCAL_TWT_DATA_SOURCE = "@{LOCALTWDATASOURCE}"
var TIMEFILTER = "Last 1 months"
---[AP_CSV]---
BEGIN AP_CSV
	---Begin Main Query---
	Using UNIQE with
		DataSource = "${LOCAL_TWT_DATA_SOURCE}"
		Authentication = "IWA"
		DataAccessor = "default"
		Application = "LogAnalyzer2"
		GetSchemaTable = false
		UserId = "n/a"
		Password = "n/a"
		Site = ""
	SELECT
	  'AP'             AS Intro,
	  CHANGETIMESTAMP,
	  CHANGEUSER       AS Who,
	  AREA,
	  TABLENAME,
	  TWIROUTE         AS ROUTE,
	  TWIOPER          AS OPERID,
	  CESTATE,
	  BETATRANSFERDATE AS TxfrDate,
	  TRANSFERCOMMENTS AS Comments,
	  DELETED,
	  DATAID,
	  HISTORYID
	FROM
	  AutoProc_HISTORY
	WHERE
	  Applytimerangefilteroracle(CHANGETIMESTAMP, '${TIMEFILTER}')
	ORDER  BY
	  CHANGETIMESTAMP DESC 
END AP_CSV
---[F3_CSV]---
BEGIN F3_CSV
	---Begin Main Query---
	Using UNIQE with
		DataSource = "${LOCAL_TWT_DATA_SOURCE}"
		Authentication = "IWA"
		DataAccessor = "default"
		Application = "LogAnalyzer2"
		GetSchemaTable = false
		UserId = "n/a"
		Password = "n/a"
		Site = ""
	SELECT
	  'F3'             AS Intro,
	  CHANGETIMESTAMP,
	  CHANGEUSER       AS Who,
	  AREA,
	  TABLENAME,
	  PRODUCT,
	  ROUTE,
	  OPERID,
	  OPERTYPE,
	  TWTYPE,
	  TWUSEDFOR,
	  CESTATE,
	  BETATRANSFERDATE AS TxfrDate,
	  TRANSFERCOMMENTS AS Comments,
	  DELETED,
	  DATAID,
	  HISTORYID
	FROM
	  F3_HISTORY
	WHERE
	  Applytimerangefilteroracle(CHANGETIMESTAMP, '${TIMEFILTER}')
	ORDER  BY
	  CHANGETIMESTAMP DESC 
END F3_CSV
